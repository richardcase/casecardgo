    "version": "0.2.0",
    "configurations": [
    {
        "name": "Launch Prepaid Svc",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceRoot}/cmd/prepaid-svc",
        "args": [
            "-stderrthreshold",
            "INFO",
            "-alsologtostderr",
            "-v",
            "2"
        ]
    },
    {
        "name": "Launch Projection Svc",
        "type": "go",
        "request": "launch",
        "mode": "debug",
        "program": "${workspaceRoot}/cmd/prepaid-projector",
        "args": [
            "-stderrthreshold",
            "INFO",
            "-alsologtostderr",
            "-v",
            "2"
        ]
    }
    ]
}